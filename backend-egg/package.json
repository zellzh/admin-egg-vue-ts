{
  "name": "admin-backend-egg",
  "version": "1.0.0",
  "description": "admin's backend with eggjs",
  "private": true,
  "egg": {
    "typescript": true,
    "declarations": true
  },
  "scripts": {
    "start": "egg-scripts start --daemon --title=egg-server-backend-egg",
    "stop": "egg-scripts stop --title=egg-server-backend-egg",
    "dev": "cross-env EGG_SERVER_ENV=local egg-bin dev",
    "debug": "egg-bin debug",
    "test-local": "egg-bin test",
    "test": "npm run lint -- --fix && npm run test-local",
    "cov": "egg-bin cov",
    "tsc": "ets && tsc -p tsconfig.json",
    "ci": "npm run lint && npm run cov && npm run tsc",
    "autod": "autod",
    "lint": "eslint . --ext .ts",
    "clean": "ets clean",
    "typeorm": "node -r ts-node/register ./node_modules/typeorm/cli.js"
  },
  "dependencies": {
    "@alicloud/pop-core": "^1.7.9",
    "@hackycy/egg-typeorm": "^0.3.7",
    "bcrypt": "^5.0.0",
    "egg": "^2.6.1",
    "egg-cors": "^2.2.3",
    "egg-passport": "^2.1.1",
    "egg-passport-github": "^1.0.0",
    "egg-redis": "^2.4.0",
    "egg-scripts": "^2.6.0",
    "egg-session-redis": "^2.1.0",
    "egg-view-ejs": "^2.0.1",
    "joi": "^17.2.1",
    "jsonwebtoken": "^8.5.1",
    "mime-types": "^2.1.27",
    "mysql2": "^2.1.0",
    "node-xlsx": "^0.15.0",
    "nodemailer": "^6.4.11",
    "svg-captcha": "^1.4.0",
    "typeorm": "^0.2.28",
    "uuid": "^8.3.1"
  },
  "devDependencies": {
    "@types/jsonwebtoken": "^8.5.0",
    "@types/mime-types": "^2.1.0",
    "@types/mocha": "^2.2.40",
    "@types/node": "^14.14.0",
    "@types/nodemailer": "^6.4.0",
    "@types/supertest": "^2.0.0",
    "autod": "^3.0.1",
    "autod-egg": "^1.1.0",
    "cross-env": "^7.0.2",
    "egg-bin": "^4.11.0",
    "egg-ci": "^1.8.0",
    "egg-mock": "^3.16.0",
    "eslint": "^6.7.2",
    "eslint-config-egg": "^8.0.0",
    "ts-node": "^9.0.0",
    "tslib": "^1.9.0",
    "typescript": "^3.0.0"
  },
  "engines": {
    "node": ">=8.9.0"
  },
  "ci": {
    "version": "8"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "eslintIgnore": [
    "coverage"
  ],
  "author": "zellzh",
  "license": "MIT"
}
